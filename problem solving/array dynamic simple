pragma solidity ^0.8.0;
contract Ram{



uint[] public num;

function push(uint _num)public{
    num.push(_num);
} 
function pop()public {
num.pop();
}
function len()public view returns(uint){
    return (num.length);
}


function del(uint _index)public   {
    delete  num[_index];
}

function retarr()public  view returns(uint[] memory){
    return num;
}
function remove(uint _index)public {
    num[_index]=num[num.length-1];
    num.pop();
}

}
